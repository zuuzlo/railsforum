<h5>Welcome to the project template!</h5>
<h1>Posts</h1>
<% @posts.each do |p| %>
  <p>
    <%= link_to p.title, p %> by <%= link_to p.user.username, user_path(p.user) %> [
    <%= link_to 'Show', p %> | 
    <%= link_to 'Edit', edit_post_path(p) %> | 
    <%= link_to 'Destroy', p, method: :delete, data: { confirm: 'Are you sure?' }%> ]
  </p>
<% end %>

